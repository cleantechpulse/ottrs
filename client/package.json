{
  "name": "ottrs-client",
  "version": "1.0.0",
  "description": "ottrs client including visualization",
  "main": "index.js",
  "scripts": {
    "build-css": "lessc src/styles/style.less src/styles/style.css",
    "watch-css": "npm run build-css && less-watch-compiler src/styles src/styles style.less",
    "start-js": "react-scripts start",
    "start": "concurrently \"npm run watch-css\" \"npm run start-js\"",
    "build": "react-scripts build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tanykim/ottrs.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tanykim/ottrs/issues"
  },
  "homepage": "https://github.com/tanykim/ottrs#readme",
  "dependencies": {
    "d3": "^4.7.4",
    "lodash": "^4.17.4",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "bootstrap-css-only": "^3.3.7",
    "concurrently": "^3.4.0",
    "font-awesome": "^4.7.0",
    "less": "^2.7.2",
    "less-watch-compiler": "^1.4.2",
    "open-color": "^1.5.1",
    "react-scripts": "^0.6.0"
  }
